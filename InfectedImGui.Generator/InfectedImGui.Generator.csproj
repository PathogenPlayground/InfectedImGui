<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>

    <!-- Nullable being enabled is required for the issue to happen. -->
    <Nullable>enable</Nullable>

    <!--
      ClangSharp.Pathogen requires Windows x64 right now
      This is also indirectly a workaround for https://github.com/microsoft/ClangSharp/issues/118#issuecomment-598305888
    -->
    <PlatformTarget>x64</PlatformTarget>
    <RuntimeIdentifier>win-x64</RuntimeIdentifier>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\external\Biohazrd\Biohazrd.CSharp\Biohazrd.CSharp.csproj" />
    <ProjectReference Include="..\external\Biohazrd\Biohazrd.OutputGeneration\Biohazrd.OutputGeneration.csproj" />
    <ProjectReference Include="..\external\Biohazrd\Biohazrd.Transformation\Biohazrd.Transformation.csproj" />
    <ProjectReference Include="..\external\Biohazrd\Biohazrd.Utilities\Biohazrd.Utilities.csproj" />
    <ProjectReference Include="..\external\Biohazrd\Biohazrd\Biohazrd.csproj" />
  </ItemGroup>

</Project>
